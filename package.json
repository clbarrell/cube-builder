{
  "name": "cube-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "server": "NODE_ENV=development tsx server.ts",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run server\"",
    "build": "tsc -b && vite build",
    "build:server": "NODE_ENV=production tsc -p tsconfig.server.json --outDir dist",
    "start": "NODE_ENV=production node dist/server.js",
    "start:network": "tsx start-network.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.3",
    "@react-three/fiber": "^9.0.4",
    "@react-three/rapier": "^1.5.0",
    "@tailwindcss/vite": "^4.0.9",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "three": "^0.174.0",
    "usehooks-ts": "^3.1.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/three": "^0.174.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.7.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
